{
  "name": "@kitz/conf",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#conf": "./build/_.js",
    "#conf/*": "./build/*.js",
    "#configurator": "./build/configurator.js"
  },
  "exports": {
    ".": "./build/_.js",
    "./__": "./build/__.js"
  },
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "dev": "tsgo -p tsconfig.build.json --watch",
    "check:types": "tsgo --noEmit",
    "check:lint": "oxlint --config ../../oxlint.json src/",
    "check:package": "publint && attw --pack"
  },
  "dependencies": {
    "@kitz/core": "workspace:*",
    "type-fest": "*"
  }
}
