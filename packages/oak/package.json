{
  "name": "@kouka/oak",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#oak": "./build/_.js",
    "#oak/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js"
  },
  "dependencies": {
    "@kouka/assert": "workspace:*",
    "@kouka/cli": "workspace:*",
    "@kouka/core": "workspace:*",
    "@kouka/group": "workspace:*",
    "@kouka/tex": "workspace:*",
    "@standard-schema/spec": "*",
    "alge": "^0.8.1",
    "ansis": "*",
    "effect": "^3.0.0",
    "zod": "^4.2.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "check:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "vitest": "^4.0.16"
  }
}
