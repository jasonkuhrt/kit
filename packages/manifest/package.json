{
  "name": "@kouka/manifest",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#manifest": "./build/_.js",
    "#manifest/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js"
  },
  "dependencies": {
    "@kouka/core": "workspace:*",
    "@kouka/fs": "workspace:*",
    "@kouka/resource": "workspace:*",
    "@kouka/semver": "workspace:*",
    "effect": "^3.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "check:types": "tsc --noEmit"
  }
}
