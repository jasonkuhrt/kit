{
  "name": "@kitz/manifest",
  "version": "0.0.0",
  "description": "Manifest file utilities",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./build/_.js",
    "./__": "./build/__.js"
  },
  "dependencies": {
    "@kitz/core": "workspace:*",
    "@kitz/fs": "workspace:*",
    "@kitz/resource": "workspace:*",
    "@kitz/semver": "workspace:*",
    "effect": "^3.0.0"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "dev": "tsgo -p tsconfig.build.json --watch",
    "check:types": "tsgo --noEmit",
    "check:lint": "oxlint --config ../../oxlint.json src/",
    "check:package": "publint && attw --pack"
  }
}
