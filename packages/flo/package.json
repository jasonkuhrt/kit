{
  "name": "@kitz/flo",
  "version": "0.0.0-kitz-release",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#flo": "./build/_.js",
    "#flo/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js",
    "./__": "./build/__.js",
    "./observable": "./build/observable/__.js",
    "./visualization": "./build/visualization/__.js",
    "./workflow": "./build/workflow/__.js"
  },
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "dev": "tsgo -p tsconfig.build.json --watch",
    "check:types": "tsgo --noEmit",
    "check:lint": "oxlint --config ../../oxlint.json src/",
    "check:package": "publint && attw --pack"
  },
  "dependencies": {
    "@effect/workflow": "^0.16.0",
    "@kitz/core": "workspace:*",
    "effect": "^3.19.13"
  }
}
