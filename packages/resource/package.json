{
  "name": "@kouka/resource",
  "version": "0.0.0",
  "description": "Resource management utilities",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./build/_.js",
    "./__": "./build/__.js"
  },
  "dependencies": {
    "@effect/platform": "^0.94.0",
    "@kouka/fs": "workspace:*",
    "@kouka/jsonc": "workspace:*",
    "effect": "^3.0.0"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "dev": "tsgo -p tsconfig.build.json --watch",
    "check:types": "tsgo --noEmit",
    "check:lint": "oxlint --config ../../oxlint.json src/",
    "check:package": "publint && attw --pack"
  }
}
