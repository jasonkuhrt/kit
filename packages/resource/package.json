{
  "name": "@kouka/resource",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#resource": "./build/_.js",
    "#resource/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/__.js"
  },
  "dependencies": {
    "@effect/platform": "^0.94.0",
    "@kouka/fs": "workspace:*",
    "@kouka/jsonc": "workspace:*",
    "effect": "^3.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "check:types": "tsc --noEmit"
  }
}
