{
  "name": "@kouka/idx",
  "version": "0.0.0",
  "description": "Index data structure",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#idx": "./build/_.js",
    "#idx/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js",
    "./__": "./build/__.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "dev": "tsgo -p tsconfig.build.json --watch",
    "check:types": "tsgo --noEmit",
    "check:lint": "oxlint --config ../../oxlint.json src/",
    "check:package": "publint && attw --pack"
  },
  "dependencies": {
    "@kouka/core": "workspace:*",
    "type-fest": "^4.0.0"
  }
}
