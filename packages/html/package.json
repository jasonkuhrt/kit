{
  "name": "@kouka/html",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#html": "./build/_.js",
    "#html/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "check:types": "tsc --noEmit"
  }
}
