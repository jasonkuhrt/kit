{
  "name": "@kouka/tex",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#tex": "./build/_.js",
    "#tex/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js"
  },
  "dependencies": {
    "@kouka/color": "workspace:*",
    "@kouka/core": "workspace:*",
    "@types/node": "^25.0.3",
    "ansis": "^4.2.0",
    "effect": "^3.19.11"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "check:types": "tsc --noEmit"
  }
}
