{
  "name": "@kouka/json",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#json": "./build/_.js",
    "#json/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js"
  },
  "dependencies": {
    "@kouka/codec": "workspace:*",
    "effect": "^3.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "check:types": "tsc --noEmit"
  }
}
