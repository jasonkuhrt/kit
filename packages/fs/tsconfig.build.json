{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "paths": {
      "#fs": [
        "./src/_.js"
      ],
      "#fs/__.js": [
        "./src/__.js"
      ],
      "#fs/*": [
        "./src/*.js"
      ]
    }
  }
}
