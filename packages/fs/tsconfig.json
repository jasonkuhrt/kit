{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "build",
    "paths": {
      "#fs": [
        "./src/_.js"
      ],
      "#fs/__.js": [
        "./src/__.js"
      ],
      "#fs/*": [
        "./src/*.js"
      ]
    }
  },
  "include": [
    "src"
  ]
}
