{
  "name": "@kouka/package-manager",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#package-manager": "./build/_.js",
    "#package-manager/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js"
  },
  "dependencies": {
    "@kouka/env": "workspace:*",
    "@kouka/fs": "workspace:*",
    "@effect/platform": "*",
    "effect": "^3.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "check:types": "tsc --noEmit"
  }
}
