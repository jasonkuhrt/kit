{
  "name": "@kouka/assert",
  "version": "0.0.0",
  "description": "Assertion utilities",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#assert": "./build/_.js",
    "#assert/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js",
    "./assert": "./build/__.js"
  },
  "dependencies": {
    "@kouka/core": "workspace:*",
    "effect": "^3.19.11"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "dev": "tsgo -p tsconfig.build.json --watch",
    "check:types": "tsgo --noEmit"
  }
}
