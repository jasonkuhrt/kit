{
  "name": "@kitz/group",
  "version": "0.0.0",
  "description": "Grouping utilities",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./build/_.js",
    "./__": "./build/__.js"
  },
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "dev": "tsgo -p tsconfig.build.json --watch",
    "check:types": "tsgo --noEmit",
    "check:lint": "oxlint --config ../../oxlint.json src/",
    "check:package": "publint && attw --pack --ignore-rules no-resolution cjs-resolves-to-esm internal-resolution-error"
  },
  "dependencies": {
    "@kitz/core": "workspace:*"
  }
}
