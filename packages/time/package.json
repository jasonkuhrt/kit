{
  "name": "@kouka/time",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#time": "./build/_.js",
    "#time/*": "./build/*.js"
  },
  "exports": {
    ".": "./build/_.js"
  },
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "check:types": "tsc --noEmit"
  }
}
