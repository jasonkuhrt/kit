    "r": "libra refresh && pnpm fix:format",
    "z": "libra toggle-hiding && pnpm fix:format",
    "build": "tsc --build tsconfig.build.json",
    "dev": "pnpm run --silent build:watch:emit",
    "dev:source": "tsx",
    "build:watch:emit": "pnpm build:watch --noCheck",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "build:emit": "pnpm build --noCheck",
    "build:clean": "pnpm tsc --build tsconfig.build.json --clean && rm -rf build",
    "fix:format": "dprint fmt",
    "fix:lint": "oxlint --fix --fix-suggestions src",
    "check:format": "dprint check",
    "check:lint": "oxlint src",
    "check:types": "tsc",
    "check:types:all": "tsc --project tsconfig.all.json",
    "check:layers": "tsx scripts/check-layered-imports.ts",
    "check:package": "publint",
    "release": "GITHUB_TOKEN=$(gh auth token) dripip stable",
    "prepack": "pnpm build",
    "test:unit": "vitest",
    "test:types": "pnpm check:types && ATTEST=true vitest --run",
    "test": "pnpm test:unit && pnpm test:types",
    "bench": "tsx scripts/bench.ts",
    "bench:stats": "attest stats .",
    "bench:perf": "pnpm run '/check:types.*perf/'",
    "check:types:perf": "tsc --diagnostics",
    "fixcheck": "pnpm run '/fix:.*/' && pnpm run '/check:.*/'",
    "check": "pnpm run '/check:.*/'",
    "fix": "pnpm run '/fix:.*/'",
    "prebuild": "pnpm build:clean",
    "prepare": "husky",
    "docs:dev": "pnpm run '/docs:dev:(generate|serve)/'",
    "docs:dev:twoslash": "TWOSLASH=true pnpm run '/docs:dev:(generate|serve)/'",
    "docs:dev:generate": "tsx watch --watch-path=src --exclude '**/*.test.ts' --exclude '**/*.test-d.ts' src/utils/paka/cli.ts generate",
    "docs:dev:serve": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate": "tsx src/utils/paka/cli.ts generate",
    "generate:test-namespaces": "tsx scripts/generate-test-namespaces/index.ts",
    "toggle:precommit": "if [ -f .husky/pre-commit ]; then mv .husky/pre-commit .husky/pre-commit.disabled && echo '✗ Pre-commit hook disabled'; else mv .husky/pre-commit.disabled .husky/pre-commit && echo '✓ Pre-commit hook enabled'; fi"
