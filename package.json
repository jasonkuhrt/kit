    "release": "GITHUB_TOKEN=$(gh auth token) dripip stable",
    "prepack": "pnpm build",
    "test:unit": "vitest",
    "test:unit-with-types": "ATTEST=true vitest --run",
    "test": "pnpm test:unit && pnpm test:types",
    "bench": "tsx scripts/bench.ts",
    "bench:stats": "attest stats .",
    "bench:perf": "pnpm run '/check:types.*perf/'",
    "check:types:perf": "tsc --diagnostics",
    "fixcheck": "pnpm run '/fix:.*/' && pnpm run '/check:.*/'",
    "check": "pnpm run '/check:.*/'",
    "fix": "pnpm run '/fix:.*/'",
    "prebuild": "pnpm build:clean",
    "docs:dev": "pnpm run '/docs:dev:(generate|serve)/'",
    "docs:dev:twoslash": "TWOSLASH=true pnpm run '/docs:dev:(generate|serve)/'",
    "docs:dev:generate": "tsx watch --watch-path=src --exclude '**/*.test.ts' --exclude '**/*.test-d.ts' src/utils/paka/cli.ts generate",
    "docs:dev:serve": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate": "tsx src/utils/paka/cli.ts generate",
    "generate:test-namespaces": "tsx scripts/generate-test-namespaces/index.ts",
