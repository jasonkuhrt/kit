{
  "name": "@wollybeard/kit",
  "version": "0.0.0-dripip",
  "author": {
    "name": "Jason Kuhrt",
    "url": "https://kuhrt.me"
  },
  "sideEffects": false,
  "exports": {
    ".": "./build/exports/index.js",
    "./cache": "./build/exports/cache/index.js",
    "./codec": "./build/exports/codec/index.js",
    "./eq": "./build/exports/eq/index.js",
    "./fn": "./build/exports/fn/index.js",
    "./arr": "./build/exports/arr/index.js",
    "./fs": "./build/exports/fs/index.js",
    "./fs-layout": "./build/exports/fs-layout/index.js",
    "./fs-relative": "./build/exports/fs-relative/index.js",
    "./json": "./build/exports/json/index.js",
    "./manifest": "./build/exports/manifest/index.js",
    "./obj": "./build/exports/obj/index.js",
    "./package-manager": "./build/exports/package-manager/index.js",
    "./path": "./build/exports/path/index.js",
    "./project-controller": "./build/exports/project-controller/index.js",
    "./language": "./build/exports/language/index.js",
    "./prom": "./build/exports/prom/index.js",
    "./rec": "./build/exports/rec/index.js",
    "./str": "./build/exports/str/index.js",
    "./url": "./build/exports/url/index.js",
    "./zod-aid": "./build/exports/zod-aid/index.js",
    "./resource": "./build/exports/resource/index.js"
  },
  "scripts": {
    "r": "libra refresh && pnpm fix:format",
    "build": "tsc --project tsconfig.build.json",
    "dev": "pnpm build:watch:emit",
    "build:watch:emit": "pnpm build --noCheck --watch",
    "build:watch": "pnpm build --watch",
    "build:emit": "pnpm build --noEmit",
    "build:clean": "tsc --build --clean && rm -rf build",
    "fix:format": "dprint fmt",
    "check:format": "dprint check",
    "check:types": "tsc --noEmit",
    "release": "dripip stable",
    "prepack": "pnpm build"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@tsconfig/recommended": "^1.0.8",
    "@tsconfig/strictest": "^2.0.5",
    "dprint": "^0.49.0",
    "dripip": "^0.10.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@10.10.0",
  "files": [
    "build",
    "src"
  ],
  "libra": {
    "visibility": true
  },
  "type": "module",
  "dependencies": {
    "@types/node": "^22.15.17",
    "consola": "^3.4.2",
    "es-toolkit": "^1.37.2",
    "jsonc-parser": "^3.3.1",
    "type-fest": "^4.41.0",
    "zod": "^3.24.4",
    "zx": "^8.5.4"
  }
}
