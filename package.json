    "dprint": "^0.50.0",
    "dripip": "^0.10.0",
    "husky": "^9.1.7",
    "madge": "^8.0.0",
    "oxlint": "^1.2.0",
    "publint": "^0.3.12",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/node": "^24.0.3",
    "@zenfs/core": "^2.3.1",
    "ansis": "^4.1.0",
    "consola": "^3.4.2",
    "es-toolkit": "^1.39.3",
    "fast-check": "^4.1.1",
    "jsonc-parser": "^3.3.1",
    "pathe": "^2.0.3",
    "type-fest": "^4.41.0",
    "zod": "^3.25.67"
  },
  "libra": {
    "visibility": false,
    "exports": {
      "include": {
        "sourceCodeLayout": "^/(?<module>.+)/\\1",
        "manifestExportsEntries": {
          "./${module}": "${libra.paths.build}/${module}/${module}.js"
        }
      }
    },
    "cli": false
  },
  "madge": {
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      },
      "tsx": {
        "skipTypeImports": true
      }
    }
  }
}
