{
  "name": "@wollybeard/kit",
  "type": "module",
  "version": "0.0.0-dripip",
  "author": {
    "name": "Jason Kuhrt",
    "url": "https://kuhrt.me"
  },
  "packageManager": "pnpm@10.12.1",
  "files": [
    "build",
    "src"
  ],
  "imports": {
    "#platform:*": {
      "browser": "./src/*.browser.ts",
      "default": "./src/*.node.ts"
    },
    "#arr": "./src/arr/$.ts",
    "#arr/arr": "./src/arr/$$.ts",
    "#obj": "./src/obj/$.ts",
    "#obj/obj": "./src/obj/$$.ts",
    "#fs": "./src/fs/$.ts",
    "#fs/fs": "./src/fs/$$.ts",
    "#path": "./src/path/$.ts",
    "#path/path": "./src/path/$$.ts",
    "#str": "./src/str/$.ts",
    "#str/str": "./src/str/$$.ts",
    "#bool": "./src/bool/$.ts",
    "#bool/bool": "./src/bool/$$.ts",
    "#eq": "./src/eq/$.ts",
    "#eq/eq": "./src/eq/$$.ts",
    "#null": "./src/null/$.ts",
    "#null/null": "./src/null/$$.ts",
    "#undefined": "./src/undefined/$.ts",
    "#undefined/undefined": "./src/undefined/$$.ts",
    "#value": "./src/value/$.ts",
    "#value/value": "./src/value/$$.ts",
    "#cache": "./src/cache/$.ts",
    "#cache/cache": "./src/cache/$$.ts",
    "#codec": "./src/codec/$.ts",
    "#codec/codec": "./src/codec/$$.ts",
    "#url": "./src/url/$.ts",
    "#url/url": "./src/url/$$.ts",
    "#fn": "./src/fn/$.ts",
    "#fn/fn": "./src/fn/$$.ts",
    "#idx": "./src/idx/$.ts",
    "#idx/idx": "./src/idx/$$.ts",
    "#pat": "./src/pat/$.ts",
    "#pat/pat": "./src/pat/$$.ts",
    "#ts": "./src/ts/$.ts",
    "#ts/ts": "./src/ts/$$.ts",
    "#rec": "./src/rec/$.ts",
    "#rec/rec": "./src/rec/$$.ts",
    "#language": "./src/language/$.ts",
    "#language/language": "./src/language/$$.ts",
    "#group": "./src/group/$.ts",
    "#group/group": "./src/group/$$.ts",
    "#prom": "./src/prom/$.ts",
    "#prom/prom": "./src/prom/$$.ts",
    "#name": "./src/name/$.ts",
    "#name/name": "./src/name/$$.ts",
    "#tree": "./src/tree/$.ts",
    "#tree/tree": "./src/tree/$$.ts",
    "#test": "./src/test/$.ts",
    "#test/test": "./src/test/$$.ts",
    "#cli": "./src/cli/$.ts",
    "#cli/cli": "./src/cli/$$.ts",
    "#debug": "./src/debug/$.ts",
    "#debug/debug": "./src/debug/$$.ts",
    "#err": "./src/err/$.ts",
    "#err/err": "./src/err/$$.ts",
    "#fs-layout": "./src/fs-layout/$.ts",
    "#fs-layout/fs-layout": "./src/fs-layout/$$.ts",
    "#fs-relative": "./src/fs-relative/$.ts",
    "#fs-relative/fs-relative": "./src/fs-relative/$$.ts",
    "#http": "./src/http/$.ts",
    "#http/http": "./src/http/$$.ts",
    "#json": "./src/json/$.ts",
    "#json/json": "./src/json/$$.ts",
    "#manifest": "./src/manifest/$.ts",
    "#manifest/manifest": "./src/manifest/$$.ts",
    "#package-manager": "./src/package-manager/$.ts",
    "#package-manager/package-manager": "./src/package-manager/$$.ts",
    "#resource": "./src/resource/$.ts",
    "#resource/resource": "./src/resource/$$.ts",
    "#zod-aid": "./src/zod-aid/$.ts",
    "#zod-aid/zod-aid": "./src/zod-aid/$$.ts",
    "#*": {
      "default": "./src/*.ts"
    }
  },
  "sideEffects": false,
  "exports": {
    ".": "./build/exports/index.js",
    "./arr": "./build/arr/arr.js",
    "./bool": "./build/bool/bool.js",
    "./cache": "./build/cache/cache.js",
    "./cli": "./build/cli/cli.js",
    "./codec": "./build/codec/codec.js",
    "./debug": "./build/debug/debug.js",
    "./eq": "./build/eq/eq.js",
    "./err": "./build/err/err.js",
    "./fn": "./build/fn/fn.js",
    "./fs": "./build/fs/fs.js",
    "./fs-layout": "./build/fs-layout/fs-layout.js",
    "./fs-relative": "./build/fs-relative/fs-relative.js",
    "./group": "./build/group/group.js",
    "./http": "./build/http/http.js",
    "./idx": "./build/idx/idx.js",
    "./json": "./build/json/json.js",
    "./language": "./build/language/language.js",
    "./manifest": "./build/manifest/manifest.js",
    "./name": "./build/name/name.js",
    "./null": "./build/null/null.js",
    "./obj": "./build/obj/obj.js",
    "./package-manager": "./build/package-manager/package-manager.js",
    "./pat": "./build/pat/pat.js",
    "./path": "./build/path/path.js",
    "./prom": "./build/prom/prom.js",
    "./rec": "./build/rec/rec.js",
    "./resource": "./build/resource/resource.js",
    "./str": "./build/str/str.js",
    "./tree": "./build/tree/tree.js",
    "./ts": "./build/ts/ts.js",
    "./undefined": "./build/undefined/undefined.js",
    "./url": "./build/url/url.js",
    "./value": "./build/value/value.js",
    "./zod-aid": "./build/zod-aid/zod-aid.js"
  },
  "scripts": {
    "r": "libra refresh && pnpm fix:format",
    "z": "libra toggle-hiding && pnpm fix:format",
    "build": "tsc --build tsconfig.build.json",
    "dev": "tsx --watch",
    "dev:source": "tsx",
    "build:watch:emit": "pnpm build:watch --noCheck",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "build:emit": "pnpm build --noCheck",
    "build:clean": "pnpm tsc --build tsconfig.build.json --clean && rm -rf build",
    "fix:format": "dprint fmt",
    "fix:lint": "oxlint --fix --fix-suggestions src",
    "check:format": "dprint check",
    "check:lint": "oxlint src",
    "check:types": "tsc",
    "release": "dripip stable",
    "prepack": "pnpm build",
    "test:unit": "vitest",
    "test": "vitest",
    "fixcheck": "pnpm run '/fix:.*/' && pnpm run '/check:.*/'",
    "check": "pnpm run '/check:.*/'",
    "fix": "pnpm run '/fix:.*/'",
    "prebuild": "pnpm build:clean",
    "check:package:circular": "madge --circular --no-spinner --extensions ts,tsx ./src",
    "prepare": "husky"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^22.0.1",
    "@tsconfig/node22": "^22.0.2",
    "@tsconfig/recommended": "^1.0.10",
    "@tsconfig/strictest": "^2.0.5",
    "dprint": "^0.50.0",
    "dripip": "^0.10.0",
    "husky": "^9.1.7",
    "madge": "^8.0.0",
    "oxlint": "^1.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/node": "^24.0.3",
    "@zenfs/core": "^2.3.1",
    "ansis": "^4.1.0",
    "consola": "^3.4.2",
    "es-toolkit": "^1.39.3",
    "fast-check": "^4.1.1",
    "jsonc-parser": "^3.3.1",
    "pathe": "^2.0.3",
    "type-fest": "^4.41.0",
    "zod": "^3.25.67"
  },
  "libra": {
    "visibility": false,
    "exports": {
      "include": {
        "sourceCodeLayout": "^/(?<module>.+)/\\1",
        "manifestExportsEntries": {
          "./${module}": "${libra.paths.build}/${module}/${module}.js"
        }
      }
    },
    "cli": false
  },
  "madge": {
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      },
      "tsx": {
        "skipTypeImports": true
      }
    }
  }
}
